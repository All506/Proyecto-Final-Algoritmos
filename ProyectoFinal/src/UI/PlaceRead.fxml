<AnchorPane id="AnchorPane" prefHeight="592.0" prefWidth="775.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.PlaceReadController">
    <stylesheets>
        <URL value="@/CSS/General.css" />
    </stylesheets>
   <children>
      <AnchorPane fx:id="apRoot" layoutX="200.0" layoutY="179.0" prefHeight="404.0" prefWidth="556.0" style="-fx-background-color: #bbbbbb;" stylesheets="@../CSS/GraphButtons.css">
         <children>
            <ImageView fx:id="imageMap" fitHeight="404.0" fitWidth="556.0" layoutX="6.0" pickOnBounds="true">
               <image>
                  <Image url="@../Resources/map.jpg" />
               </image>
            </ImageView>
            <Button fx:id="btnCenter" layoutX="222.0" layoutY="188.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="1.0" text="Button" visible="false" />
            <Rectangle fx:id="rectangleGraph" arcHeight="5.0" arcWidth="5.0" fill="#3c4956" height="404.0" layoutX="4.0" layoutY="2.0" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="556.0" />
         </children>
      </AnchorPane>
      <ScrollPane fx:id="scrRadioButtons" layoutX="521.0" layoutY="5.0" prefHeight="166.0" prefWidth="235.0" stylesheets="@../CSS/General.css">
         <content>
            <VBox fx:id="vbxRadioButtons" prefHeight="166.0" prefWidth="214.0" stylesheets="@../CSS/GraphButtons.css">
               <children>
                  <RadioButton fx:id="rdbButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="205.0" text="RadioButton" />
               </children>
            </VBox>
         </content>
      </ScrollPane>
      <TableView fx:id="tblPlaces" layoutX="-1.0" layoutY="180.0" prefHeight="406.0" prefWidth="210.0" stylesheets="@../CSS/GraphButtons.css" />
      <Button fx:id="btnRandomize" layoutX="284.0" layoutY="127.0" mnemonicParsing="false" onAction="#btnRandomize" text="Randomize distances" />
      <Button fx:id="btnGenerateGraph" layoutX="284.0" layoutY="94.0" mnemonicParsing="false" onAction="#btnGenerateGraph" prefHeight="26.0" prefWidth="128.0" text="Generate graph" />
   </children>
</AnchorPane>
